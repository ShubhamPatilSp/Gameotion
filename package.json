{
  "name": "gameotion",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "npm cache clean --force",
    "server:build": "tsc -p server/tsconfig.json",
    "server:start": "node server/dist/app.js",
    "dev:server": "nodemon --watch 'server/**/*.ts' --exec 'ts-node' server/app.ts",
    "dev": "concurrently \"npm run start\" \"npm run dev:server\""
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.80.2",
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@tanstack/react-query": "^5.84.2",
    "axios": "^1.7.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "immer": "^9.0.21",
    "react": "19.1.0",
    "react-native": "0.80.2",
    "react-native-gesture-handler": "^2.16.2",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-reanimated": "^3.16.1",
    "react-native-safe-area-context": "^4.6.3",
    "react-native-screens": "^4.15.3",
    "react-native-vector-icons": "^10.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "zustand": "^4.5.7"
